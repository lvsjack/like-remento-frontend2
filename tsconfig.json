{
 "compilerOptions": {
  "target": "ES2020",
  "module": "nodenext",
  "lib": [
   "dom",
   "dom.iterable",
   "esnext"
  ],
  "allowJs": true,
  "skipLibCheck": true,
  "strict": false,
  "forceConsistentCasingInFileNames": true,
  "noEmit": true,
  "esModuleInterop": true,
  "moduleResolution": "node",
  "resolveJsonModule": true,
  "isolatedModules": true,
  "jsx": "preserve",
  "incremental": true,
  // "baseUrl": ".",
  "paths": {
   "@/*": [
    "./*"
   ]
  },
  "plugins": [
   {
    "name": "next"
   }
  ]
 },
 "include": [
  "next-env.d.ts",
  "**/*.ts",
  "**/*.tsx",
  ".next/types/**/*.ts"
 ],
 "exclude": [
  "node_modules"
 ]
}
